# Server Configuration
PORT=3000

# MongoDB Connection
# Get this from MongoDB Atlas - replace with your connection string
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority

# Ticketmaster API
# Get this from Ticketmaster Developer Portal - https://developer.ticketmaster.com/
TICKETMASTER_API_KEY=your_ticketmaster_api_key_here

# Optional: Add any other environment variables your app needs below
# JWT_SECRET=your_jwt_secret_here
# NODE_ENV=development 